<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ithan.parallaxscrollexample" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.example.ithan.parallaxscrollexample.MultipleParallaxExpandableListView"
            android:label="@string/app_name" >
        </activity>

        <activity
            android:name="com.example.ithan.parallaxscrollexample.SingleParallaxExpandableListView"
            android:label="@string/app_name" >
        </activity>

        <activity
            android:name="com.example.ithan.parallaxscrollexample.SingleParallaxListView"
            android:label="@string/app_name" >
        </activity>

        <activity
            android:name="com.example.ithan.parallaxscrollexample.MultipleParallaxListView"
            android:label="@string/app_name" >
        </activity>

        <activity
            android:name="com.example.ithan.parallaxscrollexample.SingleParallaxScrollView"
            android:label="@string/app_name" >
        </activity>

        <activity
            android:name="com.example.ithan.parallaxscrollexample.MultipleParallaxScrollView"
            android:label="@string/app_name" >
        </activity>

        <activity
            android:name="com.example.ithan.parallaxscrollexample.SingleParallaxAlphaScrollView"
            android:label="@string/app_name" >
        </activity>

    </application>

</manifest>
